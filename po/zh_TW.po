msgid "Date: %s\n"
msgstr "日期：%s\n"

#: ../src/engine/rfc822/rfc822-utils.vala:272
msgid "To: %s\n"
msgstr "收件者：%s\n"

#: ../src/engine/rfc822/rfc822-utils.vala:275
msgid "Cc: %s\n"
msgstr "副本：%s\n"

